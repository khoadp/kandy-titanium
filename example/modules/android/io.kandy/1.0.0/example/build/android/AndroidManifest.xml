<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example" android:versionCode="1" android:versionName="1.0">
	<uses-sdk android:minSdkVersion="14" android:targetSdkVersion="22"/>
	<application android:icon="@drawable/appicon" android:label="KandyExample" android:name="KandyexampleApplication" android:debuggable="false" android:theme="@style/Theme.AppCompat">
		<activity android:name=".KandyexampleActivity" android:label="@string/app_name" android:theme="@style/Theme.Titanium" android:configChanges="keyboardHidden|orientation|screenSize">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
